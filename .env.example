# LiveKit Configuration
LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=
LIVEKIT_API_SECRET=

# Server Configuration
PORT=4000
# App environment (used for feature-gating and "dev-only" behavior)
# Allowed values: dev | staging | production
APP_ENV=dev

# Optional: Node/third-party ecosystem environment
NODE_ENV=

# Optional: Agent name to dispatch (must match the Python agent's registration)
AGENT_NAME=local-test

# Session store backend
# "local" (default) or "redis"
SESSION_STORE_PROVIDER=local
# Required when SESSION_STORE_PROVIDER=redis
REDIS_URL=
# Optional Redis key prefix + TTL (seconds)
REDIS_SESSION_KEY_PREFIX=session:
REDIS_SESSION_TTL_SECONDS=

# Recording storage backend
# "local" (default) or "s3"
RECORDING_STORAGE_PROVIDER=local
# Local recordings dir when RECORDING_STORAGE_PROVIDER=local
RECORDINGS_DIR=data/recordings
# Required when RECORDING_STORAGE_PROVIDER=s3
S3_RECORDINGS_BUCKET=
S3_REGION=
# Optional S3 settings
S3_RECORDINGS_KEY_PREFIX=recordings/
S3_ENDPOINT=
S3_FORCE_PATH_STYLE=
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=
S3_SESSION_TOKEN=

# Optional: MongoDB connection string for persistence (Mongoose)
# If unset, `/agents` will return 503 and sessions can still run with inline configs.
# Example local: mongodb://localhost:27017/livekit_backend?directConnection=true
MONGODB_URI=
# Optional: database name to use when MONGODB_URI authenticates against /admin.
# Example: factory_dev
MONGODB_DATABASE=

# Telephony Management (Telnyx inbound)
# AES-256-GCM encryption key for provider API keys (32 bytes, base64 encoded)
# Generate: node -e "console.log(require('crypto').randomBytes(32).toString('base64'))"
TELEPHONY_SECRETS_KEY=
# LiveKit SIP endpoint host (e.g., abc123.sip.livekit.cloud)
LIVEKIT_SIP_HOST=
# Telnyx API key (dev/test convenience; real keys are stored encrypted in DB)
TELNYX_API_KEY=
# Plivo credentials (for live tests)
PLIVO_AUTH_ID=
PLIVO_AUTH_TOKEN=
PLIVO_TEST_PHONE_NUMBER=

# TTS voice list proxy (optional)
# Used by backend-livekit to proxy provider voice lists for agent-studio-ui.
CARTESIA_API_KEY=
CARTESIA_VERSION=2025-04-16
ELEVENLABS_API_KEY=
DEEPGRAM_API_KEY=
RIME_API_KEY=

# Inworld TTS (optional)
# Base64 credentials from Inworld Portal (Settings â†’ API Keys).
# Backend will send: Authorization: Basic <INWORLD_BASE_64>
INWORLD_BASE_64=
