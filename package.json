{
    "name": "livekit-backend",
    "version": "1.0.0",
    "description": "LiveKit backend with dynamic agent dispatch",
    "type": "module",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "pnpm build && vitest run --exclude tests/telnyxLive.test.ts --exclude tests/twilioLive.test.ts --exclude tests/plivoLive.test.ts --exclude tests/telnyxOnboardingLive.test.ts --exclude tests/twilioOnboardingLive.test.ts --exclude tests/plivoOnboardingLive.test.ts",
        "test:watch": "pnpm build && vitest",
        "test:telnyx-live": "pnpm build && vitest run tests/telnyxLive.test.ts",
        "test:twilio-live": "pnpm build && vitest run tests/twilioLive.test.ts",
        "test:plivo-live": "pnpm build && vitest run tests/plivoLive.test.ts",
        "test:telnyx-onboarding-live": "pnpm build && vitest run tests/telnyxOnboardingLive.test.ts",
        "test:twilio-onboarding-live": "pnpm build && vitest run tests/twilioOnboardingLive.test.ts",
        "test:plivo-onboarding-live": "pnpm build && vitest run tests/plivoOnboardingLive.test.ts",
        "test:onboarding-live": "pnpm build && vitest run tests/telnyxOnboardingLive.test.ts tests/twilioOnboardingLive.test.ts tests/plivoOnboardingLive.test.ts",
        "test:live": "pnpm build && vitest run tests/telnyxLive.test.ts tests/twilioLive.test.ts tests/plivoLive.test.ts tests/telnyxOnboardingLive.test.ts tests/twilioOnboardingLive.test.ts tests/plivoOnboardingLive.test.ts",
        "test:integration": "pnpm build && vitest run tests/*.integration.test.ts",
        "test:contract": "pnpm build && vitest run tests/*.contract.test.ts",
        "test:mutation": "pnpm build && stryker run",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check ."
    },
    "keywords": [
        "livekit",
        "agents",
        "voice-ai"
    ],
    "author": "",
    "dependencies": {
        "@axiomhq/pino": "^1.4.0",
        "@livekit/protocol": "^1.44.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.0",
        "express": "^5.2.1",
        "livekit-server-sdk": "^2.14.1",
        "mongoose": "^9.1.5",
        "multer": "^2.0.2",
        "pino": "^10.3.0",
        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@stryker-mutator/core": "^9.5.1",
        "@stryker-mutator/typescript-checker": "^9.5.1",
        "@stryker-mutator/vitest-runner": "^9.5.1",
        "@types/cors": "^2.8.0",
        "@types/express": "^5.0.6",
        "@types/multer": "^2.0.0",
        "@types/node": "^20.0.0",
        "@types/supertest": "^6.0.3",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-unicorn": "^63.0.0",
        "globals": "^17.1.0",
        "knip": "^5.83.1",
        "prettier": "^3.8.1",
        "supertest": "^7.1.4",
        "tsx": "^4.0.0",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.53.1",
        "vitest": "^3.2.4"
    }
}
