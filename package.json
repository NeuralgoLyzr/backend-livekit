{
  "name": "livekit-backend",
  "version": "1.0.0",
  "description": "LiveKit backend with dynamic agent dispatch",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "pnpm build && vitest run",
    "test:watch": "pnpm build && vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "livekit",
    "agents",
    "voice-ai"
  ],
  "author": "",
  "dependencies": {
    "@livekit/agents": "^1.0.19",
    "@livekit/protocol": "latest",
    "@livekit/rtc-node": "latest",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^5.2.1",
    "livekit-server-sdk": "^2.14.1",
    "mongoose": "^9.1.5",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.0",
    "@types/express": "^5.0.6",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.1.0",
    "prettier": "^3.8.1",
    "supertest": "^7.1.4",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.53.1",
    "vitest": "^3.2.4"
  }
}